#!/bin/sh

echo "/*\n    Auto-generated by gensym." > symbols.c
echo "    DO NOT EDIT\n*/\n\n#include \"symbols.h\"\n#include <stdint.h>\n" >> symbols.c
echo "symbol_t _kernel_symtab[]={" >> symbols.c
nm $1 | grep " T " | awk '{ print "    { .addr = 0x"$1", .name = \""$3"\" }," }' | sort >> symbols.c
echo "    { .addr = UINT64_MAX, .name = \"\" }\n};" >> symbols.c
