#!/bin/sh

echo "/*\n    Auto-generated by gensym." > _symbols.c
echo "    DO NOT EDIT\n*/\n\n#include \"symbols.h\"\n#include <stdint.h>\n" >> _symbols.c
echo "const symbol_t _kernel_symtab[] = {" >> _symbols.c
nm $1 | grep " T " | awk '{ print "    { .addr = 0x"$1", .name = \""$3"\" }," }' | sort >> _symbols.c
echo "    { .addr = UINT64_MAX, .name = \"\" }\n};" >> _symbols.c
